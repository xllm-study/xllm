{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "start": "vite",
    "server": "tsx watch server/index.ts",
    "build": "npm run build:fe && npm run build:be && zip -r xllm.zip dist",
    "build:fe": "vite build",
    "build:be": "esbuild server/index.ts --platform=node --target=node22.11 --format=cjs --out-extension:.js=.cjs --outdir=dist --bundle  && cp -r server/drizzle dist",
    "lint": "eslint .",
    "lint:tsc": "tsc -b",
    "preview": "vite preview",
    "fmt": "prettier . --write",
    "db:push": "drizzle-kit push"
  },
