    "@tailwindcss/vite": "^4.1.10",
    "@tanstack/react-query": "^5.80.7",
    "@trpc/client": "^11.4.1",
    "@trpc/server": "^11.4.1",
    "@trpc/tanstack-react-query": "^11.4.1",
    "canvas-confetti": "^1.9.3",
    "cmdk": "^1.1.1",
